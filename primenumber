#include<iostream>
using namespace std;

bool isPrime(int n)
{
    int count=0;
    for(int i = 2 ; i < n ; i ++) 
    {
        if(n%i == 0)
        {
            count=count+1;
            break;
        }
    }
      
    if(count==1){
    return false;
    
    }
    else{
        return true;
    }
}

int primeFactors(int n)
{
    for(int i = 2; i <= n ; i ++)
    {
        if(isPrime(i))
        {
            int x = i ; 
            while(n%x==0)
            {
                cout << i << " " ;
                x = x*i ; 
            }
        }
    }
}

int main()
{
    int n;
    cin>>n;
    primeFactors(n);
    return 0;
}
